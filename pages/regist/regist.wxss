.app {
	font-size: 14px;
}
.app .element {
  display: block;
	width: auto;
  max-width: 100%;
  box-sizing: border-box;
  -moz-box-sizing:border-box;
  -webkit-box-sizing:border-box;
  background-size: 100% 100%;
  background-repeat: no-repeat;
  z-index: 1;
  border-color: #222222;
  border-width: 0;
}
.app .title {
  text-align: center;
  margin-top: 10px;
}
.app .title text{
  color: #D0D0D0;
}
.app .text {
  max-width: 100%;
  min-height: 10px;
  line-height: 1.4em;
  white-space: pre-wrap;
  word-break: break-all;
  height: auto !important;
}
.app .list-vessel-wrap .text, .app .dynamic-vessel .text, .app .form-vessel .text {
  white-space: normal;
}
.app .form-vessel {
	height: auto;
	min-height: 45px;
	padding-top: 10px;
}
.app .input-ele {
	width: 60%;
	height: 30px;
	margin-left: auto;
  margin-right: auto;
}
.app .input-ele input {
  width: 100%;
  height: 100%;
	box-sizing: border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	border-radius: inherit;
	border: 1px solid #d3d3d3;
  text-indent: 6px;
  outline: none;
  background-color: #fff;
}
.input-ele.must:after,
.textarea-ele.must:after,
.upload-img.must:after,
.xcx-time-ele.must:after,
.grade-ele.must:after,
.select-ele.must .title:after {
  content: '*';
  position: absolute;
  right: -10px;
  top: 0;
  font-size: 16px;
  color: red;
}
.app .style {
    width: 642.1875rpx;
    
    border-radius: 9.375rpx;
    height: 93.75rpx;
    opacity: 1;
    margin-left: auto;
}
.page_btn_register{
  background-color:#66cc99;
  color:white;
}
.page_btn{
  display: flex;
}
.app .apartment {
   margin-top: 50.4375rpx;
   margin-bottom: 0rpx;
}
.app .apartment text{
  color: grey;
  font-size: 15px;
}
.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
}
.title-view{
  bottom:50px;
  font-size: 12pt;
  margin-bottom: 50px;
  width: 100%;
  
  
  height: fit-content; 
  
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.toast-view{
  height: fit-content; 
  width: fit-content;
  padding-bottom: 10px;
  padding-top: 10px;
  padding-left: 20px;
  padding-right: 20px;
  border:1px #A9A9A9 solid;
  border-radius: 10px;
  color: #ffffff;
  background-color: #A9A9A9;
  text-align: center;
}
.car-radio{
  margin-top: 0px;
}
.car-radio text{
  color: grey;
  font-size: 15px;
}
.weui-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
          color: grey;
}
.weui-cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}

.driver_infor_label{
    margin-left: 10px;
}
.weui-cell:before {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #D9D9D9;
  color: #D9D9D9;
  left: 15px;
}
.bright789_view_hide{
    display: none;
}
.bright789_view_show{
    display: block;
}